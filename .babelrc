{
  "env": {
    "development": {
      "presets": [
        ["es2015"], "react", "stage-3"
      ],
      "plugins": [
        'transform-class-properties',
        'transform-decorators-legacy'
      ]
    },
    "es": {
      "presets": [
        ["es2015", { "modules": false }], "react", "stage-3"
      ],
      "plugins": [
        'transform-class-properties',
        'transform-decorators-legacy'
      ]
    }
  }
}
